<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <!-- The CPU temperature divider, if needed -->
    <integer name="config_cpuTempDivider" translatable="false">1000</integer>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">7</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">8</integer>

    <!-- Max visible system icons -->
    <integer name="config_maxVisibleStatusIconContainer">9</integer>

    <!-- Default rounded corner curve (a Bezier). Must match (the curved path in) rounded.xml -->
    <string name="config_rounded_mask" translatable="false">M122,0 C71.76,3.59 52.03,8.07 29.6,29.6 8.07,52.03 3.59,71.76 0,122</string>
</resources>
